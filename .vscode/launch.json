    "version": "0.2.0",
    "configurations": [
       
        {
            "name": "CONS",
            "type": "debugpy",
            "request": "launch",
            "program": "consume.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": ["--mode=SW",
                    "--epoch_size=3",
                    "--probe_metrics=RTT,INBOUND,OUTBOUND",
                    "--batch_size=4",
                    "--mitigation",
                    "--no_proxy_host"]   
        }
    ]
}